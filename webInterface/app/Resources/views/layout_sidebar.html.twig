{% extends "::base.html.twig" %}

{% set body_css_classes="hold-transition skin-blue fixed sidebar-mini" %}

{% block body %}
<div class="wrapper">
	<header class="main-header">
    	<a class="logo" href="{{ path('homepage') }}">
    		<span class="logo-mini">
    			{{ app_name }}
    		</span>
    		{{ app_name }}
    	</a>
    	<nav class="navbar navbar-static-top">
    		<a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
    			<span class="sr-only">Toggle navigation</span>
    			<span class="icon-bar"></span>
        		<span class="icon-bar"></span>
        		<span class="icon-bar"></span>
    		</a>
    		<div class="navbar-custom-menu">
    			<ul class="nav navbar-nav">
    				{% block beforeUserDropDownNav %}
    				{% endblock %}
    				<li class="dropdown user user-menu">
    					<a href="#" class="dropdown-toggle" data-toggle="dropdown">
    						<img class="user-image" src="data:image/svg+xml;utf8;base64," />
	    					<span class="hidden-xs">{{ app.user.username }}</span>
    					</a>
    					<ul class="dropdown-menu">
    						<li class="user-header">
    							<img class="img-circle" alt="User Image" src="data:image/svg+xml;utf8;base64," />
    							<p>{{ app.user.username }}</p>
    						</li>
    						<li class="user-body">
    							<div class="row">
    								<div class="col-xs-6 text-center">
	    								<a class="btn btn-flat btn-default" href="{{ path('fos_user_profile_show') }}">Profile view</a>
    								</div>
    								<div class="col-xs-6 text-center">
	    								<a class="btn btn-flat btn-default" href="{{ path('fos_user_profile_edit') }}">Profile edit</a>
    								</div>
    							</div>
    						</li>
    						<li class="user-footer">
    							<a class="btn btn-block btn-flat btn-danger" style="color:#fff" href="{{ path('fos_user_security_logout') }}"><i class="fa fa-power-off"></i>Logout</a>
    						</li>
    					</ul>
    				</li>
    				{% block afterUserDropDownNav %}
    				{% endblock %}
    			</ul>
    		</div>
    	</nav>
	</header>
	<aside class="main-sidebar">
		<section class="sidebar">
			{% block sidebar %}
			{% endblock %}
		</section>
	</aside>
	<div class="content-wrapper">
			{% block contentPageHeader %}
				<section class="content-header">
					{% block contentHeaderContent %}
						<h1>{% block title %} {% endblock %}</h1>
					{% endblock %}
				</section>
			{% endblock %}
			<section class="content">
				{% block pageContent %}
				{% endblock %}
			</section>
		</div>
	<footer class="main-footer">
		<div>Icons made by <a href="http://www.flaticon.com/authors/dave-gandy" title="Dave Gandy">Dave Gandy</a> from <a href="http://www.flaticon.com" title="Flaticon">www.flaticon.com</a> is licensed by <a href="http://creativecommons.org/licenses/by/3.0/" title="Creative Commons BY 3.0" target="_blank">CC 3.0 BY</a></div>
	</footer>
</div>

{% endblock %}

{% block stylesheets %}
	{{ parent() }}
	<link rel="stylesheet" type="text/css" href="{{asset('assets/vendor/font-awesome/css/font-awesome.css')}}" >
{% endblock %}

{% block javascriptsFooter %}
	{{ parent() }}
	<script src="{{ asset('assets/vendor/adminlte/app.min.js') }}"></script>
{% endblock %}